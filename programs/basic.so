
	put 0 i         @ i = 0

start:            @ def start():
	jump total      @     total()
	add i 1 i       @     i += 1
	lth i 10 b      @     b = i < 10
	jmpf b start    @     if not b: start()

	end             @ exit()

total:            @ def total():
	inn x           @     x = int(input())
	inn y           @     y = int(input())
	add x y z       @     z = x + y
	out z           @     print(z)
	back            @     return
